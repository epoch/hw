<% @artists.each do |artist| %>
  <div class='box'>
      <%= image_tag(artist.image, :class => 'thumb') %><br>
      <%= link_to(artist.name, artist) %>
      <%= link_to('Edit', edit_artist_path(artist)) %>
      <%= link_to('Delete', artist, :data => {:confirm => 'Are you sure?', :method => :delete }) %>
  </div>
<% end %>